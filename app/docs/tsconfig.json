    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "pages/_app.tsx",
    ".next/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ],
  "compilerOptions": {
    "paths": {
